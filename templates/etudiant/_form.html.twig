{{ form_start(form) }}

<table align="center" class="table">
  <tbody>
    <tr>
      <td class="intitule"><label for="etudiant_nom" class="required">Nom</label></td>
      <td class="champ">{{ form_widget(form.nom) }}</td>
    </tr>
    <tr>
      <td class="intitule"><label for="etudiant_prenom" class="required">Prénom</label></td>
      <td class="champ">{{ form_widget(form.prenom) }}</td>
    </tr>
    <tr>
      <td class="intitule"><label for="etudiant_mail" class="required">Mail</label></td>
      <td class="champ">{{ form_widget(form.mail) }}</td>
    </tr>
    <tr>
      <td class="intitule"><label for="etudiant_estDemissionaire" class="required">Démissionaire</label></td>
      <td class="champ">{{form_widget(form.estDemissionaire)}}</td>
    </tr>
  </tbody>
</table>

<div class='bouton'>
  <a href="{{ path('etudiant_index') }}" class="d-none d-sm-inline-block btn btn-sm btn-secondary shadow-sm bouton">Annuler</a>
  <button class="d-none d-sm-inline-block btn btn-sm btn-primary shadow-sm">{{ button_label|default('Enregistrer') }}</button>
</div>

{{ form_end(form) }}
